<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Protobuf Include="protos\*.proto" GrpcServices="Both" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.Tools" Version="2.27.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ketchup.Consul" Version="1.0.0" />
    <PackageReference Include="Ketchup.Core" Version="0.1.0" />
    <PackageReference Include="Ketchup.Grpc" Version="0.1.0" />
    <PackageReference Include="Ketchup.Profession" Version="0.1.4" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Services\Operate\" />
    <Folder Include="Services\User\" />
    <Folder Include="Services\Role\" />
  </ItemGroup>

</Project>
